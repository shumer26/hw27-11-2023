# work 1
from drawbot_skia.drawbot import rect, saveImage, fill
y = 0
step = 150
fill (0, 255, 255)
for i in range (7):
    rect (450, y, 100, 100)
    y = y + step
saveImage('work1.pdf')

# work 2
from drawbot_skia.drawbot import rect, saveImage, fill
y = 60
step = 150
fill (1, 0, 0)
for i in range (10):
    x = 150 
    for j in range(10):
        rect (y, x, 50, 50)
        x = x + step
    y = y + step
saveImage ('work2.pdf')
